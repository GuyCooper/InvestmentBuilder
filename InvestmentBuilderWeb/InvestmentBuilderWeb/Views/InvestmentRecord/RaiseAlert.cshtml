<div id="errorAlert" class="alert alert-danger alert-dismissable"> 
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"> 
        &times; 
    </button>
     <h3>@ViewBag.AlertTitle</h3>
     <ul>
         @foreach(var error in ViewBag.AlertList)
         {
             <li>@error</li>
         }
     </ul>
</div>
